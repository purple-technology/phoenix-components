{
	"name": "@purple/phoenix-components",
	"version": "4.23.1",
	"description": "",
	"main": "dist/bundle.umd.js",
	"module": "dist/bundle.esm.js",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"engines": {
		"node": ">=12"
	},
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/purple-technology/phoenix-components"
	},
	"keywords": [
		"react",
		"forms",
		"styled-components"
	],
	"contributors": [
		"Ales Chromec <achromec@purple-technology.com>",
		"Adam Stepanek <astepanek@purple-technology.com>"
	],
	"license": "MIT",
	"scripts": {
		"lint": "eslint 'src' '.storybook' --ext .ts,.tsx,.js,.jsx,.json,.d.ts",
		"lint:fix": "eslint 'src' '.storybook' --ext .ts,.tsx,.js,.jsx,.json,.d.ts --fix",
		"build": "rollup -c && rm -r ./dist/types",
		"build-watch": "rollup -c -w",
		"prepare": "husky install",
		"prebuild": "rimraf dist",
		"dev": "npm run storybook",
		"storybook": "start-storybook -p 6006",
		"storybook:build": "build-storybook -c .storybook",
		"predeploy": "npm run storybook:build",
		"release:feature": "standard-version && git push --follow-tags",
		"release:publish": "npm run build && npm publish --access public && npm run deploy-storybook",
		"deploy-storybook": "npm run storybook:build && cp -r .circleci storybook-static && storybook-to-ghpages -e=storybook-static",
		"snapshot": "build-storybook -c .storybook && percy storybook ./storybook-static",
		"percy": "if [[ $(git rev-parse --symbolic-full-name --abbrev-ref HEAD) != \"master\" ]]; then percy storybook ./storybook-static; else echo \"ERROR: percy should not be run on master because these snapshots are auto-approved\"; fi;",
		"test": "jest"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,json,d.ts}": "eslint --fix"
	},
	"dependencies": {
		"@react-hook/previous": "~1.0.1",
		"@tippyjs/react": "~4.2.5",
		"@types/react-pdf": "~5.0.5",
		"countries-and-timezones": "~2.3.1",
		"is-mobile": "~3.0.0",
		"lodash": "~4.17.21",
		"nanoid": "~3.1.23",
		"nouislider": "~15.2.0",
		"react-day-picker": "~7.4.10",
		"react-dropzone": "~10.2.0",
		"react-inlinesvg": "~2.3.0",
		"react-pdf": "~5.3.2",
		"react-select": "~4.3.1",
		"react-tabs": "~3.2.2",
		"styled-system": "~5.1.5"
	},
	"peerDependencies": {
		"react": ">=16.3",
		"react-dom": ">=16.3",
		"styled-components": "^5.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.4.4",
		"@babel/preset-env": "^7.4.4",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.15.0",
		"@commitlint/cli": "^16.0.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@percy/cli": "^1.0.0-beta.71",
		"@percy/storybook": "^4.1.0",
		"@pxblue/storybook-rtl-addon": "^1.0.1",
		"@rollup/plugin-node-resolve": "^13.0.4",
		"@rollup/plugin-typescript": "^8.2.1",
		"@rollup/plugin-url": "^6.0.0",
		"@storybook/addon-a11y": "^6.4.12",
		"@storybook/addon-docs": "^6.4.12",
		"@storybook/addon-essentials": "^6.4.12",
		"@storybook/addon-links": "^6.4.12",
		"@storybook/addon-viewport": "^6.4.12",
		"@storybook/react": "^6.4.12",
		"@storybook/storybook-deployer": "^2.8.10",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.2",
		"@types/countries-and-timezones": "^2.0.3",
		"@types/jest": "^27.0.2",
		"@types/lodash": "^4.14.178",
		"@types/react": "^17.0.6",
		"@types/react-dom": "^17.0.5",
		"@types/react-select": "^4.0.15",
		"@types/react-tabs": "^2.3.3",
		"@types/styled-components": "^5.1.9",
		"@types/styled-system": "^5.1.13",
		"@typescript-eslint/eslint-plugin": "^4.8.2",
		"@typescript-eslint/parser": "^4.8.2",
		"babel-core": "^7.0.0-bridge.0",
		"babel-loader": "^8.0.5",
		"eslint": "^7.27.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.23.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"formik": "^2.1.4",
		"husky": "^7.0.1",
		"jest": "^27.2.4",
		"lint-staged": "^12.0.0",
		"prettier": "2.3.2",
		"react": "^17.0.2",
		"react-docgen-typescript-loader": "^3.1.1",
		"react-dom": "^17.0.2",
		"rimraf": "^3.0.0",
		"rollup": "^2.63.0",
		"rollup-plugin-dts": "^4.0.0",
		"rollup-plugin-peer-deps-external": "^2.2.0",
		"rollup-plugin-styles": "^3.14.1",
		"rollup-plugin-terser": "^7.0.2",
		"standard-version": "^9.3.0",
		"storybook-addon-designs": "^6.0.0",
		"styled-components": "^5.3.0",
		"tslib": "^2.2.0",
		"typescript": "^4.5.4"
	}
}
