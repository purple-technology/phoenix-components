{
	"source": ["my-axiory-tokens/web/global.json"],
	"platforms": {
		"js": {
			"transformGroup": "js",
			"buildPath": "src/tokens/",
			"files": [
				{
					"destination": "tokens.json",
					"format": "json/nested"
				}
			]
		},
		"scss": {
			"transformGroup": "scss",
			"buildPath": "src/tokens/",
			"files": [
				{
					"destination": "tokens.scss",
					"format": "customFormat",
					"options": {
						"showFileHeader": false
					}
				}
			]
		}
	}
}
